(set-logic UFLIA)
(declare-fun f (Int) Int)
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Int)
(declare-fun d () Int)
(assert (forall ((x Int) (y Int)) (or (not (< x y)) (< (f x) (f y)))))
(assert (and (and (< a b) (< c d)) (not (and (< (f a) (f b)) (< (f c) (f d))))))
(check-sat)
(exit)